# Server
SERVER_PORT=8080

# Database
POSTGRES_USER=chaekpool
POSTGRES_PASSWORD=your_postgres_password_here
POSTGRES_DB=chaekpool_app_local
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password_here

# JWT
JWT_SECRET=your_jwt_secret_key_at_least_32_characters_long_here
JWT_ACCESS_TOKEN_VALIDITY_SECONDS=900
JWT_REFRESH_TOKEN_VALIDITY_SECONDS=604800

# OAuth (Kakao)
KAKAO_CLIENT_ID=your_kakao_client_id
KAKAO_CLIENT_SECRET=your_kakao_client_secret
KAKAO_REDIRECT_URI=http://localhost:8080/api/v1/auth/oauth/kakao/callback

# CORS
CORS_ALLOWED_ORIGINS=http://localhost:8080,http://localhost:3000

# Monitoring (Optional - Override if needed)
PROMETHEUS_PORT=9090

LOKI_PORT=3100

JAEGER_UI_PORT=16686
JAEGER_OTLP_HTTP_PORT=4318
JAEGER_OTLP_GRPC_PORT=4317
JAEGER_LOG_LEVEL=info

GRAFANA_PORT=3000
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=admin
GRAFANA_LOG_LEVEL=info
